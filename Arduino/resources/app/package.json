        "buildDate": "2022-10-27T13:26:40.924Z"
      }
    },
    "backend": {
      "config": {
        "configDirName": ".arduinoIDE",
        "singleInstance": true
      }
    },
    "generator": {
      "config": {
        "preloadTemplate": "./resources/preload.html"
      }
    }
  },
