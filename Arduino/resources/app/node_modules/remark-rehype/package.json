{
  "name": "remark-rehype",
  "version": "10.1.0",
  "description": "remark plugin that turns markdown into HTML to support rehype",
  "license": "MIT",
  "repository": "remarkjs/remark-rehype",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/unified"
  },
  "author": "Titus Wormer <tituswormer@gmail.com> (https://wooorm.com)",
  "sideEffects": false,
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "lib/",
    "index.d.ts",
    "index.js"
  ],
  "dependencies": {
    "@types/hast": "^2.0.0",
    "@types/mdast": "^3.0.0",
    "mdast-util-to-hast": "^12.1.0",
    "unified": "^10.0.0"
  },
  "devDependencies": {
    "@types/tape": "^4.0.0",
    "c8": "^7.0.0",
    "prettier": "^2.0.0",
    "rehype-stringify": "^9.0.0",
    "remark-cli": "^10.0.0",
    "remark-parse": "^10.0.0",
    "remark-preset-wooorm": "^9.0.0",
    "remark-stringify": "^10.0.0",
    "rimraf": "^3.0.0",
    "tape": "^5.0.0",
    "type-coverage": "^2.0.0",
    "typescript": "^4.0.0",
    "xo": "^0.47.0"
  },
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "bracketSpacing": false,
    "semi": false,
    "trailingComma": "none"
  },
  "remarkConfig": {
    "plugins": [
      "preset-wooorm"
    ]
  },
  "typeCoverage": {
    "atLeast": 100,
    "detail": true,
    "strict": true,
    "ignoreCatch": true,
    "#": "needed `any`s",
    "ignoreFiles": [
      "lib/index.d.ts",
      "lib/index.js",
      "index.d.ts"
    ]
  }
}